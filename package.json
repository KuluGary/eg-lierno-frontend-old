{
  "name": "lierno-client-app",
  "productName": "Lierno",
  "description": "Lierno frontend application",
  "author": "Gabriel CuÃ©tara",
  "main": "public/electron.js",
  "homepage": "./",
  "version": "0.1.6",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/KuluGary/eg-lierno-frontend.git"
  },
  "build": {
    "appId": "com.lierno.app",
    "files": [
      "./build/**/*",
      "./public/electron.js"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ]
    },
    "nodeGypRebuild": "false",
    "npmRebuild": "false"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-build": "npm run build && npm electron-builder",
    "release": "electron-builder --publish=always",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "dependencies": {
    "@material-ui/core": "^4.9.13",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.1.2",
    "electron-updater": "^4.3.1",
    "iframe-resizer-react": "^1.0.3",
    "jwt-decode": "^2.2.0",
    "leaflet": "^1.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-leaflet": "^2.7.0",
    "react-modal-image": "^2.5.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "electron": "^8.2.5",
    "electron-builder": "^19.26.3",
    "wait-on": "^5.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
